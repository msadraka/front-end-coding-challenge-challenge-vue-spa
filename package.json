    "npm": "please use yarn to run this project"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite --port 3000",
    "build": "vue-tsc && vite build",
    "preview": "vite preview --port 3000",
    "format": "prettier ./**/*.{vue,ts,js,css,scss,md,json} --write",
    "lint": "eslint --ext './**/*.{vue,ts,js}' --fix",
    "lint-staged": "lint-staged",
    "test:unit": "vitest"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "primevue": "^3.41.1",
    "vue": "^3.3.8",
    "vue-query": "^1.26.0",
    "vue-router": "^4.2.5",
    "msw": "^1.3.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/vue": "^8.0.1",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.9.5",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^15.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2",
    "vite": "^5.0.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "lint-staged": {
    "*.{vue,ts,js}": [
      "eslint --fix"
    ],
    "*.{vue,ts,js,css,scss,md,json}": "prettier --write"
  }
